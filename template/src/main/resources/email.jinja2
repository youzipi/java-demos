<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
<h1>{{ title }}</h1>
<table border>
    <tr>
        <td>商户</td>
        <td>门店</td>
        <td>失效平台</td>
    </tr>
    {% for shop in shops %}
        <tr>
            <td>{{ shop.shopName }}</td>
            <td>{{ shop.accountName }}</td>
            <td>{{ shop.platform }}</td>
        </tr>
    {% endfor %}
</table>
</body>
</html>
